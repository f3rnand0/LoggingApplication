package demo.appactlog.service;

import junit.framework.Test;
import junit.framework.TestCase;
import junit.framework.TestSuite;

/**
 * Unit test for simple App.
 */
public class AppActivityServiceTest 
    extends TestCase
{
    /**
     * Create the test case
     *
     * @param testName name of the test case
     */
    public AppActivityServiceTest( String testName )
    {
        super( testName );
    }

    /**
     * @return the suite of tests being tested
     */
    public static Test suite()
    {
        return new TestSuite( AppActivityServiceTest.class );
    }

    /**
     * Rigourous Test :-)
     */
    public void testAppActivityService()
    {
        AppActivityService.log("AppA", "Activity logging test", "INFO");
    	assertTrue( true );
    }
}
